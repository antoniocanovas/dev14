<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_invoice_supplier_form_suasor_inh" model="ir.ui.view">
        <field name="name">Supplier form inh suasor</field>
        <field name="model">account.move</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="account.invoice_supplier_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date_due']" position="after">
                <field name="suasor_invoice_id" readonly="True"/>
            </xpath>
        </field>
    </record>

    <record id="view_invoice_form_suasor_inh" model="ir.ui.view">
        <field name="name">Form inh suasor</field>
        <field name="model">account.move</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="account.invoice_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date_due']" position="after">
                <field name="suasor_invoice_id" readonly="True"/>
            </xpath>
        </field>
    </record>

    <record id="suasor_invoice_form" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <form create="false" edit="false" delete="false">
                <sheet string="Suasor">
                    <h2>
                        <field name="name"/>
                    </h2>
                    <group>
                        <field name="invoice_id"/>

                    </group>
                </sheet>

            </form>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">suasor.invoice</field>
        <field name="name">Suasor Invoice Form</field>
        <field name="priority">16</field>
        <field name="type">form</field>
    </record>

