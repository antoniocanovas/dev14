        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_vehicle_dealership_customer_invoice" model="ir.ui.menu">
        <field name="action" ref="account.action_move_out_invoice_type"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">Facturas</field>
        <field name="parent_id" ref="menu_vehicle_dealership_sale"/>
        <field name="sequence">30</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_vehicle_dealership_purchase" model="ir.ui.menu">
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">Compras</field>
        <field name="parent_id" ref="menu_vehicle_dealership"/>
        <field name="sequence">50</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_vehicle_dealership_purchase_rfq" model="ir.ui.menu">
        <field name="action" ref="purchase.purchase_rfq"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">Peticiones</field>
        <field name="parent_id" ref="menu_vehicle_dealership_purchase"/>
        <field name="sequence">10</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_vehicle_dealership_purchase_confirmed" model="ir.ui.menu">
        <field name="action" ref="purchase.purchase_form_action"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">Compras</field>
        <field name="parent_id" ref="menu_vehicle_dealership_purchase"/>
        <field name="sequence">20</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_vehicle_dealership_supplier_invoice" model="ir.ui.menu">
        <field name="action" ref="account.action_move_in_invoice_type"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">Facturas</field>
        <field name="parent_id" ref="menu_vehicle_dealership_purchase"/>
