<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="action_product_outlet" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="[('dealership', '=', 'True')]"/>
        <field name="context"
               eval="{'search_default_filter_to_sell':1,'default_type':'product','default_outlet': True}"/>
        <field name="filter" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="limit">80</field>
        <field name="name">VehicleDealership</field>
        <field name="res_model">product.template</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">kanban,tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0),
        (0, 0, {'view_mode': 'kanban', 'view_id': ref('product.product_template_kanban_view')}),
        (0, 0, {'view_mode': 'tree', 'view_id': ref('product_template_vehicle_tree')}),]"/>
    </record>

    <record id="action_fleet_vehicle_serie" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="context">{}</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="limit">80</field>
        <field name="name">Serie</field>
        <field name="res_model">fleet.vehicle.serie</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="usage" eval="False"/>
        <field name="view_id" eval="False"/>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="menu_fleet_vehicle_serie" model="ir.ui.menu">
        <field name="action" ref="action_fleet_vehicle_serie"/>
        <field name="active" eval="True"/>
        <field name="name">Serie</field>
        <field name="parent_id" ref="fleet.fleet_configuration"/>
        <field name="sequence">3</field>
    </record>

    <menuitem
            action="action_product_outlet"
            id="menu_vehicle_dealership"
            name="Vehicle dealership"
            sequence="10"
            web_icon="product_outlet,static/icon.png"
    />

    <record id="menu_vehicle_dealership_vehicle" model="ir.ui.menu">
        <field name="action" ref="action_product_outlet"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">Veh√≠culos</field>
        <field name="parent_id" ref="menu_vehicle_dealership"/>
        <field name="sequence">10</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_vehicle_dealership_contact" model="ir.ui.menu">
        <field name="action" ref="contacts.action_contacts"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">Contactos</field>
        <field name="parent_id" ref="menu_vehicle_dealership"/>
        <field name="sequence">20</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_vehicle_dealership_crm" model="ir.ui.menu">
        <field name="action" ref="crm.crm_lead_action_pipeline"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">CRM</field>
        <field name="parent_id" ref="menu_vehicle_dealership"/>
        <field name="sequence">30</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_vehicle_dealership_sale" model="ir.ui.menu">
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">Ventas</field>
        <field name="parent_id" ref="menu_vehicle_dealership"/>
        <field name="sequence">40</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_vehicle_dealership_quotation" model="ir.ui.menu">
        <field name="action" ref="sale.action_quotations_with_onboarding"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">Presupuestos</field>
        <field name="parent_id" ref="menu_vehicle_dealership_sale"/>
