<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="scrap_unbuild_res_company_form_view" model="ir.ui.view">
        <field name="name">scrap.unbuild.res.company.form.view</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <data>
                <field name="website" position="after">
                    <field name="unbuild_location_id"/>
                    <field name="refurbish_location_id"/>
                    <field name="unbuild_manager_id"/>
                </field>
            </data>
        </field>
    </record>

</odoo>
