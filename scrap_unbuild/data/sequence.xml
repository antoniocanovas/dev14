<?xml version="1.0" ?>
<odoo>
    <data>

        <record id="scrap_unbuild_sequence" model="ir.sequence">
            <field name="name">Unbuild Sequence</field>
            <field name="prefix">IDR</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="code">scrap.unbuild</field>
            <field name="implementation">no_gap</field>
        </record>

        <record id="scrap_unbuild_sequence_standard_product" model="ir.sequence">
            <field name="name">Standard Product</field>
            <field name="prefix">RV</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="code">scrap.unbuild.standard</field>
            <field name="implementation">no_gap</field>
        </record>

        <record id="scrap_unbuild_sequence_subproduct" model="ir.sequence">
            <field name="name">Standard Product</field>
            <field name="prefix">RS</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="code">scrap.unbuild.subproduct</field>
            <field name="implementation">no_gap</field>
        </record>

        <record id="scrap_unbuild_sequence_other_product" model="ir.sequence">
            <field name="name">Other Product</field>
            <field name="prefix"></field>
            <field name="padding">7</field>
            <field name="number_increment">1</field>
            <field name="code">scrap.unbuild.other</field>
            <field name="implementation">no_gap</field>
        </record>

    </data>
</odoo>
