<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="group_roadmap_user" model="res.groups">
        <field name="name">User</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_admin'))]"/>
        <!-- <field name="rule_groups" eval="[(4, ref('roadmap_user'))]"/> -->
        <field name="category_id" ref="module_category_roadmap"/>
    </record>

    <record id="group_roadmap_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="implied_ids" eval="[(4, ref('group_roadmap_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_admin'))]"/>
        <!-- <field name="rule_groups" eval="[(4, ref('roadmap_manager'))]"/> -->
        <field name="category_id" ref="module_category_roadmap"/>
    </record>

</odoo>