<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <menuitem
            action="action_view_project_roadmap"
            id="roadmap_main"
            name="Roadmap"
            sequence="3"
            web_icon="project_roadmap,static/description/icon.png"
    />

  <record id="menu_roadmap_projects" model="ir.ui.menu">
    <field name="action" ref="roadmap_kanban_project_act_window"/>
    <field name="active" eval="True"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="name">Projects</field>
    <field name="parent_id" ref="roadmap_main"/>
    <field name="sequence">10</field>
    <field name="web_icon" eval="False"/>
  </record>

  roadmap_kanban_project_view

  <record id="menu_project_roadmap" model="ir.ui.menu">
    <field name="action" ref="action_view_project_roadmap"/>
    <field name="active" eval="True"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="name">Roadmap</field>
    <field name="parent_id" ref="roadmap_main"/>
    <field name="sequence">20</field>
    <field name="web_icon" eval="False"/>
  </record>

  <record id="menu_project_roadmap_starred" model="ir.ui.menu">
    <field name="action" ref="action_view_project_roadmap_starred"/>
    <field name="active" eval="True"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="name">Starred</field>
    <field name="parent_id" ref="roadmap_main"/>
    <field name="sequence">30</field>
    <field name="web_icon" eval="False"/>
  </record>
