<?xml version="1.0" encoding="utf-8"?>
<odoo>
 <data>
       <!-- create system parameter fields in pos restaurant -->
	   <record id="subscription_code" model="ir.config_parameter">
	        <field name="key">skit_pos.subscription_code</field>
	        <field name="value">M1</field>
       </record>
       <record id="subscription_email_address" model="ir.config_parameter">
	        <field name="key">skit_pos.subscription.mail</field>
	        <field name="value">demo@srikeshinfotech.com</field>
       </record>
       <record id="product_expiry_date" model="ir.config_parameter">
	        <field name="key">skit_pos.expiry_date</field>
	        <field name="value">2022-04-14</field>
       </record>
       <record id="product_encryption_key" model="ir.config_parameter">
	        <field name="key">skit_pos.encryption_key</field>
	        <field name="value">5fa7042654ee1f3c9c04f176fc548c9c5fadca96</field>
       </record>
       <record id="license_active" model="ir.config_parameter">
	        <field name="key">skit_pos.is_license_active</field>
	        <field name="value">True</field>
       </record>
       <record id="base.ir_config_list_action" model="ir.actions.act_window">
	        <field name="domain" eval="[('id','not in', [ref('skit_pos_restaurant.subscription_code'), 
	        							 ref('skit_pos_restaurant.subscription_email_address'),
	        							 ref('skit_pos_restaurant.product_expiry_date'),
	        							 ref('skit_pos_restaurant.product_encryption_key'),
	        							 ref('skit_pos_restaurant.license_active')])]"/>
	   </record>

  </data>
</odoo>