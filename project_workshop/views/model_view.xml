<?xml version="1.0" encoding="utf-8"?>
<odoo>
      <record id="project_workshop_form" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <form name="project_workgroup_main_form">
                <sheet string="Entradas">
                    <h2>
                        <field name="name"/>
                    </h2>
                    <group name="project_workgroup_general">
                        <field name="note"/>
                        <field name="project_id" attrs="{'invisible':[('is_new','=',True)]}"/>
                        <field name="project_partner_id" attrs="{'invisible':[('is_new','=',True)]}"/>
                        <field name="is_new" widget="boolean_toggle"/>
                        <field name="license" attrs="{'invisible':[('is_new','=',False)]}"/>
                        <field name="partner_id" attrs="{'invisible':[('is_new','=',False)]}"/>
                        <field name="model" attrs="{'invisible':[('is_new','=',False)]}"/>
                        <field name="user_id" attrs="{'invisible':[('is_new','=',False)]}"/>
                    </group>
                    <!--
                    <footer>
                        <button t-if="invoice_id" name="create_work_shop" string="Ver Factura" class="oe_right oe_highlight"
                                type="object"/>
                    </footer>-->
                </sheet>
            </form>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">project.workshop</field>
        <field name="name">Default form</field>
        <field name="priority">16</field>
        <field name="type">form</field>
    </record>

    <record id="project_workshop_tree" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="project_id"/>
                <field name="project_partner_id"/>
                <field name="user_id"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">project.workshop</field>
        <field name="name">Default tree</field>
        <field name="priority">16</field>
        <field name="type">tree</field>
    </record>
</odoo>