<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="product_dictionary_default_form" model="ir.ui.view">
        <field name="name">product.dictionary.default.form</field>
        <field name="model">product.dictionary</field>
        <field name="arch" type="xml">
            <form>
                <sheet string="Unbuild Wizard">
                    <h2>
                        <field name="name" required="1"/>
                    </h2>
                    <group>
                        <field name="type"/>
                        <field name="categ_id"/>
                    </group>
                </sheet>
            </form>
        </field>
        <field name="type">form</field>
    </record>

    <record id="product_dictionary_default_tree" model="ir.ui.view">
        <field name="name">Product dictionary default tree</field>
        <field name="model">product.dictionary</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name"/>
                <field name="categ_id"/>
                <field name="type"/>
            </tree>
        </field>
        <field name="type">tree</field>
    </record>

</odoo>
