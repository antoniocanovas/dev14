<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="action_scrap_category" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="filter" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="limit">80</field>
        <field name="name">SCRAP Category</field>
        <field name="res_model">scrap.category</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="menu_scrap_category" model="ir.ui.menu">
        <field name="action" ref="action_scrap_category"/>
        <field name="active" eval="True"/>
        <field name="name">SCRAP Categories</field>
        <field name="parent_id" ref="crm.crm_menu_config"/>
        <field name="sequence">50</field>
    </record>

</odoo>
