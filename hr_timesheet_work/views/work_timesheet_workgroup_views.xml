<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="timesheet_workgroup_default_form" model="ir.ui.view">
        <field name="name">Default timesheet workgrouop form view</field>
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <form>
                <sheet string="Timesheet Workgroup">
                    <h4>
                        <field name="name" widget="text"/>
                    </h4>
                    <group>
                        <field name="date" attrs="{'readonly':[('analytic_line_ids','!=',[])]}"/>
                        <field name="hour_laboral"/>
                        <field name="hour_extra"/>
                        <field name="task_ids"/>
                        <field name="employee_ids"/>
                        <field name="analytic_line_ids"/>
                        <field name="work_sheet_id" readonly="1"/>
                    </group>
                </sheet>
            </form>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">work.timesheet.workgroup</field>
        <field name="priority">16</field>
        <field name="type">form</field>
    </record>

    <record id="timesheet_workgroup_default_tree" model="ir.ui.view">
        <field name="name">Default timesheet Workgroup tree view</field>
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree name="timesheet_workgroup" default_order="date desc">
                <field name="name"/>
                <field name="work_sheet_id" optional="hide"/>
                <field name="hour_laboral"/>
                <field name="hour_extra"/>
                <field name="date" readonly="1"/>
                <field name="task_ids" widget="many2many_tags" optional="hide"/>
                <field name="employee_ids" widget="many2many_tags" optional="hide"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">work.timesheet.workgroup</field>
        <field name="priority">16</field>
        <field name="type">tree</field>
    </record>

