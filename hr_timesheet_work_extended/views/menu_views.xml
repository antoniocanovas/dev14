<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="action_hr_timesheet_work" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="limit">80</field>
        <field name="name">Work Base</field>
        <field name="res_model">work.base</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_hr_timesheet_work" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="limit">80</field>
        <field name="name">Work</field>
        <field name="res_model">timesheet.work</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
            action="action_hr_timesheet_work"
            id="menu_hr_timesheet_work"
            name="Work Base"
            sequence="10"
            web_icon=""
    />

    <record id="menu_wb_bar" model="ir.ui.menu">
        <field name="action" ref="action_hr_timesheet_work"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [ref('base.group_user')])]"/>
        <field name="name">Work Base</field>
        <field name="parent_id" ref="menu_hr_timesheet_work"/>
        <field name="sequence">1</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_hr_timesheet_work" model="ir.ui.menu">
        <field name="action" ref="action_hr_timesheet_work"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [ref('base.group_user')])]"/>
        <field name="name">Work</field>
        <field name="parent_id" ref="menu_hr_timesheet_work"/>
        <field name="sequence">2</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_hr_timesheet_work_task" model="ir.ui.menu">
        <field name="action" ref="project.action_view_all_task"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [ref('base.group_user')])]"/>
        <field name="name">Task</field>
        <field name="parent_id" ref="menu_hr_timesheet_work"/>
        <field name="sequence">3</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_wb_repairs" model="ir.ui.menu">
        <field name="action" ref="repair.action_repair_order_tree"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [ref('base.group_user')])]"/>
        <field name="name">Repairs</field>
        <field name="parent_id" ref="menu_hr_timesheet_work"/>
        <field name="sequence">4</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="menu_wb_mrp" model="ir.ui.menu">
        <field name="action" ref="mrp.mrp_workorder_todo"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [ref('base.group_user')])]"/>
        <field name="name">MRP</field>
        <field name="parent_id" ref="menu_hr_timesheet_work"/>
        <field name="sequence">5</field>
        <field name="web_icon" eval="False"/>
    </record>



</odoo>
