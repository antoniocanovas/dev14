<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_company_filestore" model="ir.ui.view">
        <field name="name">filestore.delete</field>
        <field name="model">res.company</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[1]" position="after">
                <page name="FSD" string="FSD">
                    <group>
                        <button name="check_attachment_data" string="Force GET FS" type="object" class="oe_highlight oe_read_only"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
